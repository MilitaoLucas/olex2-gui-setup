<cif_customisation
  <translation
    <item from="_symmetry_cell_setting" to="_space_group_crystal_system">
    <item from="_symmetry_space_group_name_Hall" to="_space_group_name_Hall">
    <item from="_symmetry_space_group_name_H-M" to="_space_group_name_H-M_alt">
    <item from="_symmetry_Int_Tables_number-M" to="_space_group_IT_number">
    <item from="_diffrn_reflns_av_sigmaI/netI" to="_diffrn_reflns_av_unetI/netI">
    <item from="_diffrn_radiation_source" to="_diffrn_source">
    <item from="_publ_contact_*,_publ_contact_author_id_orcid" to="_audit_contact_*">
    <item from="_publ_author*" to="_audit_author*">
  >
  <sorting
    <pivots
      <pivot "_audit_creation">
      <pivot "_shelxl_version_number"> <pivot "_shelx_SHELXL_version_number">
      <pivot "_audit">
      <pivot "_publ">
      <pivot "_chemical_name">  <pivot "_chemical_formula">  <pivot "_chemical">
      <pivot "_atom_type">
      <pivot "_shelx_space_group">
      <pivot "_space_group">  <pivot "_space_group_symop">
      <pivot "_symmetry">
      <pivot "_cell_length">  <pivot "_cell_angle">  <pivot "_cell_volume">  <pivot "_cell_formula">  <pivot "_cell">
      <pivot "_shelx_estimated_absorpt_T">
      <pivot "_exptl_">
      <pivot "_diffrn_reflns">  <pivot "_diffrn">
      <pivot "_reflns">
      <pivot "_computing">
      <pivot "_refine"> <pivot "_olex2_refinement_description">
      <pivot "_atom_sites">  <pivot "_atom_site">  <pivot "_atom_site_aniso">
      <pivot "_geom_special">  <pivot "_geom_bond">  <pivot "_geom_angle">  <pivot "_geom">
      <pivot "_smtbx">
      <pivot "_shelx_res_file"> <pivot "_shelx_res_checksum">
      <pivot "_shelx_hkl_file"> <pivot "_shelx_hkl_checksum">
      <pivot "_shelx_fab_file"> <pivot "_shelx_fab_checksum">
      <pivot "_platon">
      <pivot "_shelx">
      <pivot "_refln">
      <pivot "_iucr">
    >
    <endings
      <ending "_h_min">
      <ending "_h_max">
      <ending "_k_min">
      <ending "_k_max">
      <ending "_l_min">
      <ending "_l_max">
      <ending "_min">
      <ending "_max">
    >
  >
  <skip_merge
    <_diffrn_radiation_wavelength>
    <_diffrn_radiation_type>
    <_diffrn_radiation_xray_symbol>
    <_refine_ls_*>
    <_refine_diff_*>
    <_diffrn_reflns_*>
    <_diffrn_measured_fraction_*>
    <_exptl_crystal_density_diffrn>
    <_exptl_crystal_F_000>
    <_chemical_formula_sum>
    <_diffrn_oxdiff_ac3_digest_hkl>
    <_diffrn_oxdiff_ac3_digest_frames>
    <_diffrn_oxdiff_ac3_digest_frames>
>
  <do_merge
   <_*_details>
  >
  <export_metacif
    <_cell_measurement_*>
    <_exptl_absorpt_*>
    <_chemical_name_*>
    <_exptl_crystal_*>
    <_*_details>
    <_iucr_refine_fcf_details skip="true">
    <_diffrn_measurement_*>
    <_diffrn_absorpt_*>
    <_diffrn_crystal_*>
    <_diffrn_detector_*>
    <_computing_*>
    <_olex2_*>
    <_local_*>
    <_publ_*>
    <_refine_* skip="true">
    <_exptl_crystal_F_000 skip="true">
    <_exptl_crystal_density_diffrn skip="true">
    <_exptl_absorpt_coefficient_mu skip="true">
    <_geom_special_details skip="true">
    <_reflns_special_details skip="true">
    <_olex2_refinement_description skip="true">
    <_iucr_refine_instructions_details skip="true">
    <_smtbx_masks_void>
    <_platon_squeeze_void>
    <_platon_squeeze_*>
    <_smtbx_masks_*>
  >
  <#include "app.ConfigDir()/cif_customisation.xld" extend='false'>
>


diagnostics {
  hkl{
    MeanIOverSigma{
      op = "greater"
        .type = str
      display = "Mean I/&sigma;"
        .type = str
      value_format= "1f"
        .type = str
      grade1  = 20
        .type = float
      grade2 = 15
        .type = float
      grade3 = 10
        .type = float
      grade4 = 5
        .type = float
      top = 25
        .type = float
    }
    Rint{
      op = "smaller"
        .type = str
      display = "R<sub>int</sub>"
        .type = str
      value_format= "2%"
        .type = str
      grade1  = 0.04
        .type = float
      grade2 = 0.06
        .type = float
      grade3 = 0.10
        .type = float
      grade4 = 0.15
        .type = float
      top = 0.15
        .type = float
    }
  }
}

<!-- SECOND ROW -->
<!-- #include tool-row gui\blocks\tool-row.htm;name='METADATA_DATES';1 -->
    <font size="2">
    <td align="right" width="80%">
      <table cellpadding='0' cellspacing='0' width='100%'>
        <tr>
    <td VALIGN="center" ALIGN="left" width="14%">
      <b>Submitted</b>
    </td>
    <td width='17%'>
      <input
        type="date"
        dropdown=True
        height="GetVar('HtmlInputHeight')"
        width='100%'
        name="SET_SNUM_REPORT_DATE_SUBMITTED"
        value="spy.Getparam(snum.report.date_submitted)"
        valign="center"
        onchange="spy.SetParam('snum.report.date_submitted',html.GetValue('~name~'))>>spy.AddVariableToUserInputList('snum.report.date_submitted')>>spy.changeBoxColour('~name~','#FFDCDC')"
        bgcolor="spy.bgcolor('~name~')"
      >
    </td>
    <td VALIGN="center" ALIGN="right" width="17%">
      <b>Collected</b>
    </td>
    <td width='17%'>
      <input
        type="date"
        dropdown=True
        height="GetVar('HtmlInputHeight')"
        width='100%'
        name="SET_SNUM_REPORT_DATE_COLLECTED"
        value="spy.Getparam(snum.report.date_collected)"
        valign="center"
        onchange="spy.SetParam('snum.report.date_collected',html.GetValue('~name~'))>>spy.AddVariableToUserInputList('snum.report.date_collected')>>spy.changeBoxColour('~name~','#FFDCDC')"
        bgcolor="spy.bgcolor('~name~')"
      >
  </td>
    <td VALIGN="center" ALIGN="right" width="18%">
      <b>Completed</b>
    </td>
    <td width='17%'>
      <input
        type="date"
        height="GetVar('HtmlInputHeight')"
        width='100%'
        name="SET_SNUM_REPORT_DATE_COMPLETED"
        value="spy.Getparam(snum.report.date_completed)"
        valign="center"
        dropdown=True
        onchange="spy.SetParam('snum.report.date_completed',html.GetValue('~name~'))>>spy.AddVariableToUserInputList('snum.report.date_completed')>>spy.changeBoxColour('~name~','#FFDCDC')"

        bgcolor="spy.bgcolor('~name~')"
      >
    </td>
        </tr>
      </table>
    </td>
    </font>


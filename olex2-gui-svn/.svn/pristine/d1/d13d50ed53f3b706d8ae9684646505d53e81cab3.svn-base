    <td VALIGN="center" ALIGN="left" width="14%">
      <b>Sample ID</b>
    </td>

    <td VALIGN="center" ALIGN="left" width="86%">
      <font size="2">
        <input
          type="text"
          width="100%"
          height="$GetVar(HtmlComboHeight)"
          name="SET_SNUM_REPORT_SUBMISSION_ORIGINAL_SAMPLE_ID"
          value="spy.GetParam('snum.report.submission_original_sample_id')"
          valign="center"
          onleave="spy.SetParam('snum.report.submission_original_sample_id',html.GetValue('~name~'))>>spy.AddVariableToUserInputList('snum.report.submission_original_sample_id')>>spy.changeBoxColour('~name~','#FFDCDC')"
          bgcolor="spy.bgcolor('~name~')"
        >
      </font>
    </td>

<!-- SECOND ROW -->
<!-- #include tool-row gui\blocks\tool-row.htm;name='METADATA_PEOPLE';1 -->
    <td VALIGN="center" ALIGN="left" width="14%">
      <b>Submitter</b>
    </td>
$+
  html.Snippet(
    GetVar(default_link),
    "name=SET_SNUM_METACIF_SUBMITTER",
    "value=spy.getPersonInfo(spy.GetParam('snum.report.submitter'), 'displayname')",
    "onclick=spy.gui.report.publication.OnPersonChange('~name~','snum.report.submitter')>>spy.gui.report.publication.OnAddNameToAuthorList(None,spy.GetParam('snum.report.submitter'))",
    "td1=<td width='36%'>"
    )
$-

    <td VALIGN="center" ALIGN="right" width="14%">
      <b>Operator</b>
    </td>

$+
  html.Snippet(
    GetVar(default_link),
    "name=SET_SNUM_METACIF_OPERATOR",
    "value=spy.getPersonInfo(spy.GetParam('snum.report.operator'), 'displayname')",
    "onclick=spy.gui.report.publication.OnPersonChange('~name~', 'snum.report.operator')>>spy.gui.report.publication.OnAddNameToAuthorList(None,spy.GetParam('snum.report.operator'))",

<!-- FIRST ROW -->
<tr name=QUICK_DRAWING_STYLES_1>
<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Quick-Drawing-Styles;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td name=IMG_BALL_AND_STICK width=10%>
    <zimg border="0" src="toolbar-ball-and-stick.png">
  </td>
  <td name=BTN_BALL_AND_STICK>
$spy.MakeHoverButton(button-balls_&_sticks,pers>>sel -u)
  </td>
  <td name=IMG_ANIS width=10%>
    <zimg border="0" src="toolbar-anis.png">
  </td>
  <td name=BTN_BALL_AND_STICK>
<!-- Button Part ELLIPSOIDS -->
  <map name="map_ellipsoids">
    <area shape="rect"
      coords="0,0,60,25"
      href="spy.InActionButton(button-ellipsoids_|_h,on)>>
refresh>>
telp>>
sel -u>>
spy.InActionButton(button-ellipsoids_|_h,off)"
    target="%Show Ellipsoids%"
    >

<!-- Button Part H Uiso -->
    <area shape="rect"
      coords="60,0,500,25"
      href="spy.InActionButton(button-ellipsoids_|_h,on)>>
refresh>>
telph>>
sel -u>>
spy.InActionButton(button-ellipsoids_|_h,off)"

      target="%Show Ellipsoids and Hydrogen atom Uiso%"
    >
  </map>
    <zimg border="0" name=IMG_BUTTON-ELLIPSOIDS_|_H src="button-ellipsoids_|_h.png" usemap=#map_ellipsoids>
</td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>


<!-- #include tool-row-help gui\blocks\tool-row-help.htm;name=FINISHING_QUICK_SORT; help_ext=QuickSort;1; -->
<td width='20%'>
  <b>QuickSort</b>
</td>
    $+
    html.Snippet(GetVar(default_link),
    "value=Sort with Current Settings",
    "onclick=spy.gui.do_sort()",
    )$-


  <td ALIGN='left' width='5%'>
$spy.gui.images.get_action_button_html('edit', "spy.gui.SwitchTool(h2-work-sorting)>>spy.gui.tools.flash_gui_control(h2-sorting)","Edit Sort Settings")
  </td>

<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->


<!-- #include tool-row-help gui\blocks\tool-row-help.htm;name=FINISHING_QUICK_IMAGES; help_ext=QuickImages;1; -->
<td width='20%'>
  <b>QuickImages</b>
</td>

    $+
    html.Snippet(GetVar(default_link),
    "value=Selected",
    "onclick=label -type=spy.GetParam('user.image.label_symm_type') -symm=spy.GetParam('user.image.label_symm')",
    )$-

    $+
    html.Snippet(GetVar(default_link),
    "value=non-H",
    "onclick=label \$*-H,Q -type=spy.GetParam('user.image.label_symm_type') -symm=spy.GetParam('user.image.label_symm')",
    )$-

    $+
    html.Snippet(GetVar(default_link),
    "value=No Labels",
    "onclick=kill labels",
    )$-

    $+
    html.Snippet(GetVar(default_link),
    "value=Go",
    "bgcolor=GetVar(gui.green)",
    "fgcolor=#ffffff",
    "onclick=spy.gui.images.MakeBitmapImage()",
    )$-

  <td ALIGN='left' width='5%'>
$spy.gui.images.get_action_button_html('edit', "spy.gui.SwitchTool(h2-tools-images)>>spy.gui.tools.flash_gui_control(h2-images)","Edit Image Settings")
  </td>


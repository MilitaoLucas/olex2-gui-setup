<!-- #include tool-top gui\blocks\tool-top.htm;image=#image;onclick=#onclick;1; -->


<!-- FIRST ROW -->
<!-- #include tool-row-help gui\blocks\tool-row-help.htm;name='NAMING';help_ext=Naming;1 -->
<table width="100%" border="0" width="100%" bgcolor="$GetVar(HtmlTableBgColour)"><tr>
  <td width="18%">%Start%</td>
  <td name=MODE_NAME_NUMBER>$+
     html.Snippet("gui/snippets/input-text", "name=naming*start", "width=35", "manage=true")
  $-</td>
  <td>%Suffix%</td>
  <td name=MODE_NAME_SUFFIX>$+
     html.Snippet("gui/snippets/input-text", "name=naming*suffix", "width=35", "manage=true")
  $-</td>
  <td>%Type%</td>
  <td name=MODE_NAME_TYPE>$+
     html.Snippet("gui/snippets/input-text", "name=naming*type", "width=35", "manage=true")
  $-</td>
  <td name=IMG_BUTTON_SMALL-NAME align='right'>
$spy.MakeHoverButton('small-Name',"spy.gui.NamingMode")
  </td>
</tr></table>
<table width="100%" border="0" width="100%" bgcolor="$GetVar(HtmlTableBgColour)"><tr>
  <td width="18%">Autocomplete</td>
  <td width='20%' align="left">$+
    html.Snippet("gui/snippets/input-checkbox", "name=naming*auto*on", "value=ON", "manage=true")
   $-</td>
  <td width='20%' align="left">$+
    html.Snippet("gui/snippets/input-checkbox", "name=naming*auto*stop_type", "value=Stop on type", "manage=true")
   $-</td>
  <td width='20%' align="left">$+
    html.Snippet("gui/snippets/input-checkbox", "name=naming*auto*stop_part", "value=Stop on part", "manage=true")
   $-</td>
  <td align="right">$+
    html.Snippet("gui/snippets/input-checkbox", "name=naming*auto*stop_branch", "value=Stop on branch", "manage=true")
   $-</td>
</tr></table>

<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->

<!-- SECCOND ROW -->
<!-- #include tool-row-help gui\blocks\tool-row-help.htm;name='AUTOMATIC_HYDROGEN_NAMING';help_ext=Automatic-Hydrogen-Naming;1 -->
  <td name=TICK_AUTO_HYDROGEN_NAMING_LABEL colspan=3>
    %Automatic Hydrogen Naming%
  </td>
  <td name=TICK_AUTO_HYDROGEN_NAMING align='right'>
    <font size="$GetVar('HtmlFontSizeControls')">
    <input
      type="checkbox"
      name = "TICK_AUTO_HYDROGEN_NAMING"
      checked="spy.GetParam('snum.auto_hydrogen_naming')"
      onclick="spy.SetParam('snum.auto_hydrogen_naming',html.GetState('~name~'))"
    >
    </font>
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->

<!-- THIRD ROW -->
<!-- #include tool-row-help gui\blocks\tool-row-help.htm;name='MATCH_NAMING';help_ext=Match-Naming;1 -->
  <td>
    <a href="match sel -n=html.GetValue('MATCH_NAME_SUFFIX') -i=html.GetState(MATCH_NAME_INVERT)">
    %Equivalent Fragments% (Z' > 1)</a>
  </td>
  <td>%Suffix%</td>
  <td>
    <font size="$GetVar('HtmlFontSizeControls')">
    <input
      type="text"
      height="$GetVar('HtmlInputHeight')"
      width="30"
      bgcolor="$GetVar('HtmlInputBgColour')"
      valign='center'
      name = "MATCH_NAME_SUFFIX"
      value = ""
    >
    </font>
  </td>
  <td>%Invert for matching%</td>
  <td align="right">$+
    html.Snippet("gui/snippets/input-checkbox", "name=MATCH_NAME_INVERT",
     "manage=true")
   $-</td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->

<!-- #include tool-footer gui\blocks\tool-footer.htm;colspan=2;1; -->
<tr name=MAIN_TOOLBAR_MODEL>

<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Toolbar-Model;1; -->

<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
<!-- #include main-toolbar-2 gui\main-toolbar-2.htm;1; -->
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>

<!-- SECOND ROW -->
<tr name=MAIN_TOOLBAR width=100%'>
<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Quicktools;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td width='5%'>
$spy.MakeHoverButton(toolbar-QC,name \$Q C)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-QH,name \$Q H)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-tidy,clean)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-killH,kill \$H)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-Q,showQ)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-H,showH)
  </td>
  <td width='5%'>
$spy.MakeHoverButton(toolbar-center,compaq>>compaq -a>>center)
  </td>
  
  <td width = '5%'>
$spy.MakeHoverButton(toolbar-ok,fixunit GetValue\(SET_Z_PRIME_FIXUNIT)>>xf.au.SetZprime\(GetValue\(SET_Z_PRIME_FIXUNIT))>>html.Update)
  </td> 
  <td width='30%' align='center'>
    <font size='$GetVar(HtmlFontSizeControls)'>
    <input
      type='text'
      width="40"
      height="$GetVar(HtmlComboHeight)"
      bgcolor="spy.GetParam(gui.html.input_bg_colour)"
      name='SET_Z_PRIME_FIXUNIT'
      valign='center'
      align='right'
      value='xf.au.GetZprime()'
      onreturn="xf.au.SetZprime(GetValue(~name~))>>html.update"
      label="Z' "
      >
    </font>
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>

<!-- THIRD ROW -->
<tr name=LABEL_COMBO_BOX>
<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Labels;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td name=LABEL_COMBO_BOX_LABEL width=20%>
    <b>%Labels%</b>
  </td>
  <td align='right' name=COMBO_LABEL_COMBO_BOX>
    <font size='$GetVar(HtmlFontSizeControls)'>
    <input
      type='combo'
      width='100%'
      height="$GetVar(HtmlInputHeight)"
      name='set_label_content'
      value=''
      items='%Toggle On/Off%<-;%Atom Names%<-l;%Crystallographic Occupancy%<-o;%Chemical Occupancy%<-co;%Parts%<-p;%Link-Code%<-v;%H Atom Labels%<-h l;%Fixed Parameters%<-f;Variables<-v;AFIX Commands<-h a;Q Peak Intensities<-qi'
      bgcolor="$GetVar(HtmlInputBgColour)"
      onchange="spy.ChooseLabelContent(GetValue(~name~))"
      readonly='readonly'
    >
    </font>
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>


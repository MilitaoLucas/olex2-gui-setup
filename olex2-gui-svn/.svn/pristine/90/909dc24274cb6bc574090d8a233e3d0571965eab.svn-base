<!-- #include tool-h3 gui\blocks\tool-h3.htm;image=#image;colspan=1;1; -->

<!-- FIRST ROW -->
<tr name=IMAGE_PS_ROW_1>
<!-- #include tool-help-column gui\blocks\tool-help-first-column.htm;help_ext=mode_name;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td><b>%Name%</b></td>
  <td name=IMAGE_PS_NAME width='15%'>
    <font size=$GetVar(HtmlFontSizeControls)>
    <input
      type="text"
      height="$GetVar(HtmlInputHeight)"
      bgcolor="$GetVar(HtmlInputBgColour)"
      valign='center'
      name="IMAGE_PS_NAME"
      width="100%"
      value="spy.GetParam(snum.image.ps.name)"
      onchange="spy.SetParam(snum.image.ps.name,GetValue(~name~))"
      manage
    >
    </font>
  </td>
  <td name=IMAGE_PS_COLOUR_LINE_LABEL align='right'>
    %Lines%
  </td>
  <td name=IMAGE_PS_COLOUR_LINE>
    <font size=$GetVar(HtmlFontSizeControls)>
    <input
      type="checkbox"
      height="$GetVar(HtmlCheckboxHeight)"
      width="$GetVar(HtmlCheckboxWidth)"
      name = "COLOR_LINE"
      width="14"
      fgcolor="$GetVar(HtmlFontColour)"
      bgcolor="$GetVar(HtmlTableBgColour)"
      oncheck="SetData(COLOR_LINE,-color_line)>>spy.SetParam(snum.image.ps.colour_line,-color_line)"
      onuncheck="SetData(COLOR_LINE,' ')>>spy.SetParam(snum.image.ps.colour_line,' ')"
      checked="$strcmp(spy.GetParam(snum.image.ps.colour_line),-color_line)"
      manage
    >
    </font>
  </td>
  <td name=IMAGE_PS_COLOUR_BOND_LABEL align='right'>
    %Bonds%
  </td>
  <td name=IMAGE_PS_COLOUR_BOND>
    <font size=$GetVar(HtmlFontSizeControls)>
    <input
      type="checkbox"
      height="$GetVar(HtmlCheckboxHeight)"
      width="$GetVar(HtmlCheckboxWidth)"
      name = "COLOR_BOND"
      fgcolor="$GetVar(HtmlFontColour)"
      bgcolor="$GetVar(HtmlTableBgColour)"
      oncheck="SetData(COLOR_BOND,-color_bond)>>spy.SetParam(snum.image.ps.colour_bond,-color_bond)"
      onuncheck="SetData(COLOR_BOND,' ')>>spy.SetParam(snum.image.ps.colour_bond,' ')"
      checked="$strcmp(spy.GetParam(snum.image.ps.colour_bond),-color_bond)"
      manage
    >
    </font>
  </td>
  <td name=IMAGE_PS_COLOUR_FILL_LABEL align='right'>
    %Fill%
  </td>
  <td name=IMAGE_PS_COLOUR_FILL>
    <font size=$GetVar(HtmlFontSizeControls)>
    <input
      type="checkbox"
      height="$GetVar(HtmlCheckboxHeight)"
      width="$GetVar(HtmlCheckboxWidth)"
      name = "COLOR_FILL"
      width="17"
      fgcolor="$GetVar(HtmlFontColour)"
      bgcolor="$GetVar(HtmlTableBgColour)"
      oncheck="SetData(COLOR_FILL,-color_fill)>>spy.SetParam(snum.image.ps.colour_fill,-color_fill)"
      onuncheck="SetData(COLOR_FILL,' ')>>spy.SetParam(snum.image.ps.colour_fill,' ')"
      checked="$strcmp(spy.GetParam(snum.image.ps.colour_fill),-color_fill)"
      manage
    >
    </font>
  </td>
  <td name=IMAGE_PS_PERSPECTIVE_LABEL align='right'>
    %Pers%
  </td>
  <td name=IMAGE_PS_PERSPECTIVE>
    <font size=$GetVar(HtmlFontSizeControls)>
    <input
      type="checkbox"
      height="$GetVar(HtmlCheckboxHeight)"
      width="$GetVar(HtmlCheckboxWidth)"
      name = "IMAGE_PS_PERSPECTIVE"
      width="17"
      fgcolor="$GetVar(HtmlFontColour)"
      bgcolor="$GetVar(HtmlTableBgColour)"
      oncheck="SetData(IMAGE_PS_PERSPECTIVE,-p)>>spy.SetParam(snum.image.ps.perspective,-p)"
      onuncheck="SetData(IMAGE_PS_PERSPECTIVE,' ')>>spy.SetParam(snum.image.ps.perspective,' ')"
      checked="$strcmp(spy.GetParam(snum.image.ps.perspective),-p)"
      manage
    >
    </font>
  </td>
  <td name=IMAGE_PS_BUTTON align='right'>
$spy.MakeHoverButton(button_small-go@ps,spy.GetPsImageInstructions\())
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>


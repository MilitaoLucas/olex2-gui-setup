<!-- SECOND ROW -->
<tr name=MAIN_TOOLBAR>
<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Quicktools;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td>
    <a 
      href="name $Q C"
      target="%Q To C-target%">
      <zimg name="qc" border="0" src="toolbar-QC.png"/>
    </a>
  </td>
  <td>
    <a 
      href="name $Q H"
      target="%Q To H-target%">
      <zimg name="qh" border="0" src="toolbar-QH.png"/>
    </a>
  </td>
  <td>
    <a 
      href="clean"
      target="%Tidy Structure-target%">
      <zimg border="0" src="toolbar-tidy.png"/>
    </a>
  </td>
  <td>
    <a 
      href="kill $H"
      target="%Kill H-target% ">
      <zimg border="0" src="toolbar-killH.png"/>
    </a>
  </td>
  <td>
    <a 
      href="showQ"
      target="%Toggle Q-target%">
      <zimg border="0" src="toolbar-Q.png"/>
    </a>
  </td>
  <td>
    <a 
      href="showH"
      target="%Toggle H-target%">
      <zimg border="0" src="toolbar-H.png"/>
    </a>
  </td>
  <td>
    <a 
      target="%center_on-target%" href="center">
      <zimg  border="0" src="toolbar-center.png">
    </a>
  </td>
  <td>
  </td>
  <td>
    <a 
      href="fixunit xf.au.GetZprime()>>updatehtml"
      target="%Fix Unit-target% ">
      <zimg border="0" src="toolbar-OK.png"/>
    </a>
  </td>
  <td>
    <input
        type='text'
        width="40"
        height="$spy.GetParam(gui.html.spin_height)"
        bgcolor="spy.GetParam(gui.html.input_bg_colour)"
        name='SET_Z_PRIME_FIXUNIT'
        label="Z'&nbsp;"
        valign='center' 
        value='xf.au.GetZprime()'
        onleave="xf.au.SetZprime(GetValue(SET_Z_PRIME_FIXUNIT))>>spy.SetParam(snum.refinement.Z_prime,GetValue(SET_Z_PRIME_FIXUNIT))>>updatehtml"
      >
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>

<!-- THIRD ROW -->
<tr name=LABEL_COMBO_BOX>
<!-- #include tool-help-first-column gui\blocks\tool-help-first-column.htm;help_ext=Labels;1; -->
<!-- #include row_table_on gui\blocks\row_table_on.htm;1; -->
  <td name=LABEL_COMBO_BOX_LABEL width=20%>
    <b>%Labels%</b>
  </td>
  <td name=COMBO_LABEL_COMBO_BOX>
    <input 
      type='combo'
      width='$eval(htmlpanelwidth()-105)'
      height="$spy.GetParam(gui.html.input_height)"
      name='set_label_content'
      value=''
      items='%Toggle On/Off%<-;%Atom Names%<-l;%Crystallographic Occupancy%<-o;%Chemical Occupancy%<-co;%Parts%<-p;%Link-Code%<-v;%H Atom Labels%<-h l;%Fixed Parameters%<-f;Variables<-v;AFIX Commands<-h a;Q Peak Intensities<-qi'
      label=''
      bgcolor="$spy.GetParam(gui.html.input_bg_colour)"
      onchange="spy.ChooseLabelContent(GetValue(set_label_content))"
      readonly='readonly'
    >
  </td>
<!-- #include row_table_off gui\blocks\row_table_off.htm;1; -->
</tr>

